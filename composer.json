{
  "name"        : "backstagetechnicalservices/website",
  "description" : "The source code of the Backstage Technical Services website.",
  "keywords"    : [],
  "license"     : "GNU GPLv2",
  "type"        : "project",
  "require"     : {
    "php": ">=7.4",
    "alaouy/youtube": "^2.0",
    "anhskohbo/no-captcha": "^3.0",
    "bugsnag/bugsnag-laravel": "^2.0",
    "components/jquery": "^3.1",
    "doctrine/dbal": "^2.5",
    "eluceo/ical": "^0.15",
    "eonasdan/bootstrap-datetimepicker": "^4.17",
    "facade/ignition": "^2.0",
    "fideloper/proxy": "~4.0",
    "fortawesome/font-awesome": "^4.7",
    "fruitcake/laravel-cors": "^2.0",
    "graham-campbell/markdown": "^11.0",
    "guzzlehttp/guzzle": "^6.2",
    "intervention/image": "^2.3",
    "laravel/framework": "^7.0",
    "laravel/slack-notification-channel": "^2.0",
    "laravel/tinker": "^2.0",
    "laravel/ui": "^2.0",
    "laravelcollective/html": "^6.0",
    "lavary/laravel-menu": "^1.7",
    "magyarjeti/laravel-lipsum": "^1.1",
    "mews/purifier": "^3.0",
    "moment/moment": "^2.17",
    "select2/select2": "^4.0",
    "spatie/laravel-backup": "^6.0",
    "spatie/laravel-db-snapshots": "^1.1",
    "twbs/bootstrap": "^3.3",
    "zendframework/zend-stdlib": "^3.2"
  },
  "require-dev" : {
    "fzaninotto/faker"         : "~1.4",
    "mockery/mockery"          : "^1.0",
    "phpunit/phpunit"          : "^8.5",
    "filp/whoops"              : "^2.0",
    "barryvdh/laravel-debugbar": "^3.0"
  },
  "autoload"    : {
    "classmap": [
      "database"
    ],
    "psr-4"   : {
      "App\\": "app/",
      "Package\\": "packages/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts"     : {
    "post-root-package-install": [
      "php -r \"file_exists('.env') || copy('.env.example', '.env');\""
    ],
    "post-create-project-cmd"  : [
      "php artisan key:generate"
    ],
    "post-install-cmd"         : [
      "Illuminate\\Foundation\\ComposerScripts::postInstall"
    ],
    "post-update-cmd"          : [
      "Illuminate\\Foundation\\ComposerScripts::postUpdate"
    ],
    "post-autoload-dump"       : [
      "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
      "@php artisan package:discover"
    ]
  },
  "config"      : {
    "preferred-install": "dist",
    "sort-packages"    : true
  },
  "repositories": [
  ]
}
